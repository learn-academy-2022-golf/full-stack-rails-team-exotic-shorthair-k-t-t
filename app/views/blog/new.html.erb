<h1> Create a new blog </h1>
<%= form_with model: @blog, local: true do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
    <%= form.label :content %>
    <%= form.text_area :content %>
    <%= form.submit 'add blog' %>
    <% end %>

<%= link_to 'Back to blog', blogs_path %>
<%= link_to 'Back to home', root_path %>


